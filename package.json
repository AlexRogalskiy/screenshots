{
    "name": "styled-screenshots",
    "version": "0.1.0",
    "description": "Automatically generate styled screenshots upon request",
    "main": "index.js",
    "license": "GPL-3.0",
    "scripts": {
        "deploy:dev": "npm run setup:chrome && env-cmd -e prod vercel",
        "deploy:prod": "npm run setup:chrome && env-cmd -e prod vercel --prod",
        "develop": "env-cmd -e dev vercel dev",
        "setup:chrome": "unzip ./node_modules/puppeteer/.local-chromium/chrome-linux.zip",
        "lint": "eslint --fix .",
        "test": "TZ=utc jest",
        "test:coverage": "jest --collectCoverage",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "env-cmd": "^10.1.0",
        "isomorphic-unfetch": "^3.1.0",
        "jsdom": "^16.4.0",
        "lodash": "^4.17.20",
        "playwright-aws-lambda": "^0.6.0",
        "playwright-chromium": "^1.8.1",
        "playwright-core": "^1.8.1",
        "puppeteer": "^7.1.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/prettier": "^2.1.5",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "@vercel/node": "^1.9.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.4",
        "ts-node": ">=9.0.0",
        "typescript": "^4.1.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexRogalskiy/screenshots.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 8.0.0"
    },
    "keywords": [
        "github-actions",
        "javascript"
    ],
    "bugs": {
        "url": "https://github.com/AlexRogalskiy/screenshots/issues"
    },
    "homepage": "https://github.com/AlexRogalskiy/screenshots",
    "author": {
        "name": "Alexander Rogalskiy",
        "url": "https://github.com/AlexRogalskiy"
    },
    "funding": "https://github.com/sponsors/AlexRogalskiy"
}
